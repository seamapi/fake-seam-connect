{
  "name": "TypeScript",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "18"
    }
  },
  "extensions": [
    "ms-vsliveshare.vsliveshare",
    "dbaeumer.vscode-eslint",
    "EditorConfig.EditorConfig",
    "esbenp.prettier-vscode"
  ],
  "forwardPorts": [3000],
  "portsAttributes": {
    "3000": { "label": "App" }
  },
  "postCreateCommand": "npm install",
  "remoteUser": "node"
}
